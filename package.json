{
  "name": "trelloapp-vue-vite-ts",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "lint": "prettier --write \"src/**/*.{ts,vue,js}\" && eslint \"**/*.{ts,vue,js}\" --fix",
    "lint:only:tests": "eslint \"cypress/**/*.js\"",
    "typecheck": "tsc --noEmit -p . && vue-tsc --noEmit",
    "selectorcheck": "./scripts/getSelectors.sh",
    "open": "cypress open",
    "run": "cypress run",
    "run:api": "cypress run --env grepTags=@api",
    "run:ui": "cypress run --env grepTags=@ui",
    "start:and:open": "start-server-and-test start http-get://localhost:3000 open",
    "start:and:run": "start-server-and-test start http-get://localhost:3000 run",
    "start:and:run:api": "start-server-and-test start http-get://localhost:3000 run:api",
    "start:and:run:ui": "start-server-and-test start http-get://localhost:3000 run:ui",
    "empty:db": "cp cypress/fixtures/databases/empty.json backend/data/database.json",
    "trash:assets": "rm -rf cypress/results && mkdir cypress/results"
  },
  "dependencies": {
    "@4tw/cypress-drag-drop": "~2.0.0",
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/vite-dev-server": "^2.0.7",
    "@cypress/vue": "^3.0.3",
    "@popperjs/core": "^2.9.3",
    "@tailwindcss/jit": "^0.1.18",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.29.3",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "concurrently": "^6.0.0",
    "connect-busboy": "^0.0.2",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.9.0",
    "json-server": "^0.16.3",
    "json-server-auth": "^1.2.1",
    "moment": "~2.29.1",
    "nocache": "^3.0.1",
    "pinia": "^2.0.0",
    "postcss": "^8.2.9",
    "prettier": "^1.19.1",
    "require-uncached": "^2.0.0",
    "sendmail": "^1.6.1",
    "socket.io": "^4.0.1",
    "socket.io-client": "^4.1.2",
    "tailwindcss": "^2.2.4",
    "test-exclude": "^6.0.0",
    "typescript": "^4.1.3",
    "v-calendar": "^3.0.0-alpha.5",
    "vite": "^2.1.5",
    "vite-plugin-istanbul": "^2.2.2",
    "vite-svg-loader": "^1.5.1",
    "vue": "^3.0.5",
    "vue-3-socket.io": "^1.0.4",
    "vue-cli-plugin-ts-paths": "~0.0.4",
    "vue-eslint-parser": "^7.10.0",
    "vue-router": "^4.0.0-0",
    "vue-socket.io-extended": "^5.0.0-alpha.5",
    "vue-tsc": "^0.28.10",
    "vue3-click-away": "^1.1.0",
    "vue3-dropzone": "^0.0.7",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "cypress": "~9.0.0",
    "cypress-grep": "~2.10.2",
    "cypress-plugin-tab": "~1.0.5",
    "cypress-real-events": "~1.5.1",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "~2.12.1",
    "eslint-plugin-no-only-tests": "~2.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "mocha-junit-reporter": "~2.0.2",
    "start-server-and-test": "~1.14.0",
    "useful-library": "~1.2.2"
  }
}
